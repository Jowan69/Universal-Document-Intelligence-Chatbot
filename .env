# Rename this file to .env and fill in your keys

# Get your free Groq key from https://console.groq.com/keys
# This must be named OPENAI_API_KEY to work with the ChatOpenAI class
OPENAI_API_KEY=gsk_replace_this_with_your_groq_key

# This is the Groq API endpoint
OPENAI_API_BASE=https://api.groq.com/openai/v1

# This is the model the chatbot will use (from Groq)
LLM_MODEL=llama-3.3-70b-versatile

# Get your free Serper key from https://serper.dev/
SERPER_API_KEY=replace_this_with_your_serper_key
